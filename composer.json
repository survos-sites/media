{
    "name": "survos-sites/sais",
    "description": "Survos Async Image Server, async images using LiipImagine and Flysystem",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-apcu": "*",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo_pgsql": "*",
        "api-platform/core": ">=4.2.6",
        "api-platform/doctrine-orm": "^4.0.18",
        "api-platform/symfony": "^4.0.18",
        "bizkit/versioning-bundle": "^1.2.1",
        "doctrine/dbal": "^4.3.4",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "doctrine/doctrine-migrations-bundle": "^3.7.0",
        "doctrine/orm": "^3.5.7",
        "easycorp/easyadmin-bundle": "^4.27.4",
        "fguillot/json-rpc": ">=1.3.1",
        "friendsofsymfony/jsrouting-bundle": "^3.6.1",
        "inspector-apm/inspector-symfony": "^1.9.0",
        "jenssegers/imagehash": "^0.11.0",
        "knplabs/knp-menu": "dev-master",
        "knplabs/knp-time-bundle": "^2.4",
        "league/color-extractor": "^0.4.0",
        "league/flysystem-aws-s3-v3": "^3.30.1",
        "league/flysystem-memory": ">=3.29",
        "liip/imagine-bundle": "^2.15.0",
        "lorisleiva/cron-translator": "^0.4.6",
        "meilisearch/meilisearch-php": "dev-main",
        "nelmio/cors-bundle": "^2.6",
        "nyholm/psr7": "^1.8.2",
        "openai-php/client": ">=0.18.0",
        "paragonie/sodium_compat": "^2.4",
        "phpdocumentor/reflection-docblock": "^5.6.4",
        "phpstan/phpdoc-parser": "^2.3.0",
        "roave/better-reflection": "^6.66",
        "stof/doctrine-extensions-bundle": "^1.14",
        "survos/babel-bundle": "^2.0.62",
        "survos/bootstrap-bundle": "^2.0.62",
        "survos/command-bundle": "^2.0.62",
        "survos/crawler-bundle": "^2.0.62",
        "survos/doc-bundle": "^2.0.62",
        "survos/ez-bundle": "^2.0.62",
        "survos/google-sheets-bundle": "^2.0.62",
        "survos/key-value-bundle": "^1.6.44",
        "survos/meili-bundle": "^2.0.62",
        "survos/sais-bundle": "^2.0.62",
        "survos/simple-datatables-bundle": "^2.0.62",
        "survos/state-bundle": "^2.0.62",
        "survos/storage-bundle": "^2.0.62",
        "survos/thumb-hash-bundle": "^2.0.62",
        "survos/tree-bundle": "^2.0.62",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.5",
        "symfony/console": "^7.3.6",
        "symfony/doctrine-messenger": "^7.3.6",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.10.0",
        "symfony/form": "^7.3.6",
        "symfony/framework-bundle": "^7.3.6",
        "symfony/http-client": "^7.3.6",
        "symfony/intl": "^7.3.5",
        "symfony/mailer": "^7.3.5",
        "symfony/mime": "^7.3.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3.3",
        "symfony/object-mapper": "^7.3.6",
        "symfony/process": "^7.3.4",
        "symfony/property-access": "^7.3.3",
        "symfony/property-info": "^7.3.5",
        "symfony/redis-messenger": "^7.3.4",
        "symfony/runtime": "^7.3.4",
        "symfony/security-bundle": "^7.3.4",
        "symfony/serializer": "^7.3.5",
        "symfony/stimulus-bundle": "^2.31.0",
        "symfony/stopwatch": "^7.4",
        "symfony/string": "^7.3.4",
        "symfony/translation": "^7.3.4",
        "symfony/twig-bundle": "^7.3.4",
        "symfony/validator": "^7.3.7",
        "symfony/web-link": "^7.3",
        "symfony/webhook": "^7.3",
        "symfony/yaml": "^7.3.5",
        "symfonycasts/verify-email-bundle": "^1.17.4",
        "twig/extra-bundle": "^3.22.1",
        "twig/twig": "^3.22.0",
        "zenstruck/class-metadata": "^1.1",
        "zenstruck/messenger-monitor-bundle": "^0.5.4"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "zenstruck/class-metadata": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default \"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "jsrouting": "bin/console fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"",
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage",
        "remove-failed": "bin/console dbal:run-sql \"delete from messenger_messages where queue_name='failed'\" ",
        "index": "php -d memory_limit=3G ./c grid:index",
        "diagram": [
            "bin/console doctrine:diagram:er --filename=assets/er.svg --exclude=doctrine_migration_versions,messenger_messages",
            "bin/console doctrine:diagram:class --filename=assets/db.svg --exclude=doctrine_migration_versions,messenger_messages"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.4"
        },
            "class-metadata": {
                "paths": [
                    "src/Entity"
                ]
            }
    },
    "require-dev": {
        "jawira/doctrine-diagram-bundle": "^1.1",
        "phpunit/phpunit": "^12.4.4",
        "roave/security-advisories": "dev-latest",
        "survos/code-bundle": "^2.0.62",
        "survos/deployment-bundle": "^2.0.62",
        "symfony/browser-kit": "^7.3.6",
        "symfony/css-selector": "^7.3.6",
        "symfony/debug-bundle": "^7.3.5",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/phpunit-bridge": "^7.3.4",
        "symfony/web-profiler-bundle": "^7.3.5",
        "zenstruck/browser": "^1.9.1"
    }
}
